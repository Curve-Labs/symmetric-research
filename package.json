{
  "name": "symmetric-research",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "contracts": "yarn workspace contracts",
    "create:stable:pool": "sh scripts/deploy_stable_pool_celo.sh",
    "deploy:test:tokens": "sh scripts/deploy_tokens_celo.sh"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.5.0"
}
